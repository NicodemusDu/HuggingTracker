{
	"name": "huggingface-save",
	"displayName": "Huggingface save",
	"version": "0.0.1",
	"description": "A basic Plasmo extension.",
	"author": "Nicodemus",
	"scripts": {
		"dev": "plasmo dev",
		"build": "plasmo build",
		"package": "plasmo package",
		"format": "biome format --write .",
		"lint": "biome lint .",
		"check": "biome check --apply ."
	},
	"dependencies": {
		"@plasmohq/storage": "^1.12.0",
		"plasmo": "0.89.1",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"tailwindcss": "3.4.1"
	},
	"devDependencies": {
		"@biomejs/biome": "^1.8.3",
		"@ianvs/prettier-plugin-sort-imports": "4.1.1",
		"@types/chrome": "0.0.258",
		"@types/node": "20.11.5",
		"@types/react": "18.2.48",
		"@types/react-dom": "18.2.18",
		"postcss": "8.4.33",
		"prettier": "3.2.4",
		"typescript": "5.3.3"
	},
	"manifest": {
		"host_permissions": ["https://*/*"],
		"permissions": ["storage"],
		"action": {
			"default_popup": "popup.html"
		},
		"options_ui": {
			"page": "history.html",
			"open_in_tab": true
		}
	}
}
